@{
    ViewBag.title = "home";
}

<!DOCTYPE html>
<html lang="pt-br" class="h-100">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>GM INVESTIMENTOS</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-wEmeIV1mKuiNpC+IOBjI7aAzPcEZeedi5yW5f2yOq55WWLwNGmvvx4Um1vskeMj0" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/js/bootstrap.bundle.min.js" integrity="sha384-JEW9xMcG8R+pH31jmWH6WWP0WintQrMb4s7ZOdauHnUtxwoG2vI5DkLtS3qm9Ekf" crossorigin="anonymous"></script>
    <script src="js/tata.js"></script>
    <script src="js/jquery-3.6.0.min.js"></script>
    <script src='js/jquery.maskMoney.js' type='text/javascript'></script>
</head>
<body class="h-100" onload="CarregarPagina('Caixa')">
    <div class="h-100 bg-light d-flex flex-column bd-highlight">
        <div class="bd-highlight">

            <!-- Barra Superior -->
            <nav class="navbar navbar-expand-lg shadow navbar-dark bg-dark">
                <div class="container-fluid">
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation"><span class="navbar-toggler-icon"></span></button>
                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="navbar-nav me-auto mb-lg-0 text-center">
                            <li class="nav-item dropdown">
                                <a class="nav-link" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false"><i class="bi bi-wallet2 pe-2"></i>Fluxo de Caixa</a>
                                <ul class="dropdown-menu rounded-0" aria-labelledby="navbarDropdown">
                                    <li><a class="dropdown-item" href="#">Gráficos</a></li>
                                    <li><a class="dropdown-item" href="#" onclick="CarregarPagina('Caixa')">Entrada / Saída</a></li>
                                </ul>
                            </li>
                        </ul>
                        <div class="d-flex row text-center">
                            <div class="col-lg-12 nav-item dropdown">
                                <button type="nav-link" class="btn btn-dark" href="#" id="confMenu" role="button" data-bs-toggle="dropdown" aria-expanded="false"><i class="bi bi-sliders pe-2"></i>Configurações</button>
                                <ul class="dropdown-menu rounded-0 w-100" aria-labelledby="confMenu">
                                    <li><a class="dropdown-item" href="#">Usuário</a></li>
                                    <li><a class="dropdown-item" href="#">Sair</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </nav>
        </div>

        <!-- Conteúdo da Página -->
        <div class="container-fluid overflow-auto h-100 bd-highlight pt-2" id="ConteudoPágina">

        </div>

        <!-- Modal Sessão Expirada -->
        <div class="modal fade bg-dark" id="MsgSessaoExpirada" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="SessaoExpirada" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered">
                <div class="modal-content rounded-0">
                    <div class="modal-header">
                        <h5 class="modal-title" id="">Sessão expirada</h5>
                    </div>
                    <div class="modal-body">
                        <h5 class="fs-5"><i class="bi bi-exclamation-circle pe-2"></i>Ops.... Essa sessão expirou, favor entrar novamente.</h5>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-success rounded-0" onclick="window.location.href = '/'">IR PARA PÁGINA DE LOGIN</button>
                    </div>
                </div>
            </div>
        </div>

    </div>
    <script src="js/Caixa.js"></script>
    <script src="js/Home.js"></script>
</body>
</html>
